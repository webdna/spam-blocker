{
	"name": "webdna/spam-blocker",
	"description": "Add regex tests to form field validation",
	"type": "craft-plugin",
	"version": "4.0.1",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"spam blocker"
	],
	"support": {
		"docs": "https://github.com/spam-blocker/spam-blocker/blob/master/README.md",
		"issues": "https://github.com/spam-blocker/spam-blocker/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "webdna",
			"homepage": "https://webdna.co.uk"
		}
	],
	"require": {
		"craftcms/cms": "^4.0.0"
